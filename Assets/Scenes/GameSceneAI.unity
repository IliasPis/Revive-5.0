          \u03B3\u03B9\u03B1 \u03C4\u03B7\u03BD \u03B7\u03B3\u03B5\u03C3\u03AF\u03B1. "
        - "\u03A5\u03C0\u03BF\u03B8\u03AD\u03C3\u03C4\u03B5 \u03CC\u03C4\u03B9 \u03BF\u03B9
          \u03B1\u03BD\u03CE\u03C4\u03B5\u03C1\u03BF\u03B9 \u03C5\u03C0\u03AC\u03BB\u03BB\u03B7\u03BB\u03BF\u03B9
          \u03B8\u03B1 \u03C7\u03B5\u03B9\u03C1\u03B9\u03C3\u03C4\u03BF\u03CD\u03BD
          \u03C4\u03B9\u03C2 \u03B2\u03B5\u03BB\u03C4\u03B9\u03CE\u03C3\u03B5\u03B9\u03C2
          \u03C4\u03C9\u03BD \u03B4\u03B9\u03B1\u03B4\u03B9\u03BA\u03B1\u03C3\u03B9\u03CE\u03BD. "
        - "\u0395\u03BD\u03B8\u03B1\u03C1\u03C1\u03CD\u03BD\u03B5\u03C4\u03AD \u03C4\u03BF\u03BD
          \u03BD\u03B1 \u03C7\u03C1\u03B7\u03C3\u03B9\u03BC\u03BF\u03C0\u03BF\u03B9\u03AE\u03C3\u03B5\u03B9
          \u03B5\u03C1\u03B3\u03B1\u03BB\u03B5\u03AF\u03B1 \u03C0\u03C1\u03BF\u03C3\u03BF\u03BC\u03BF\u03AF\u03C9\u03C3\u03B7\u03C2
          \u03B4\u03B9\u03B1\u03B4\u03B9\u03BA\u03B1\u03C3\u03B9\u03CE\u03BD \u03C0\u03C1\u03BF\u03C4\u03BF\u03CD
          \u03C0\u03B1\u03C1\u03BF\u03C5\u03C3\u03B9\u03AC\u03C3\u03B5\u03B9 \u03C4\u03B7\u03BD
          \u03B9\u03B4\u03AD\u03B1 \u03C4\u03BF\u03C5 \u03C3\u03C4\u03B7\u03BD \u03B7\u03B3\u03B5\u03C3\u03AF\u03B1. "
        - "\u03A0\u03B5\u03AF\u03C4\u03B5 \u03C4\u03BF\u03C5\u03C2 \u03BD\u03B1 \u03C0\u03B5\u03C1\u03B9\u03BC\u03AD\u03BD\u03BF\u03C5\u03BD
