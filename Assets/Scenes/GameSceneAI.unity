          \u03C0\u03B1\u03C1\u03B1\u03BA\u03BF\u03BB\u03BF\u03CD\u03B8\u03B7\u03C3\u03B7\u03C2
          \u03B3\u03B9\u03B1 \u03BD\u03B1 \u03B5\u03BD\u03C4\u03BF\u03C0\u03AF\u03C3\u03B5\u03C4\u03B5
          \u03C4\u03B1 \u03B5\u03BC\u03C0\u03CC\u03B4\u03B9\u03B1 \u03BA\u03B1\u03B9
          \u03BD\u03B1 \u03C0\u03B1\u03C1\u03AD\u03C7\u03B5\u03C4\u03B5 \u03C0\u03C1\u03CC\u03C3\u03B8\u03B5\u03C4\u03B7
          \u03BA\u03B1\u03B8\u03BF\u03B4\u03AE\u03B3\u03B7\u03C3\u03B7, \u03B5\u03AC\u03BD
          \u03C7\u03C1\u03B5\u03B9\u03AC\u03B6\u03B5\u03C4\u03B1\u03B9. "
        - "\u0395\u03BD\u03B8\u03B1\u03C1\u03C1\u03CD\u03BD\u03B5\u03C4\u03B5 \u03C4\u03BF\u03BD
          \u03C0\u03C1\u03BF\u03CA\u03C3\u03C4\u03AC\u03BC\u03B5\u03BD\u03CC \u03C4\u03BF\u03C5\u03C2
          \u03BD\u03B1 \u03C7\u03B5\u03B9\u03C1\u03B9\u03C3\u03C4\u03B5\u03AF \u03C4\u03BF
          \u03B8\u03AD\u03BC\u03B1 \u03B1\u03BD\u03B5\u03BE\u03AC\u03C1\u03C4\u03B7\u03C4\u03B1.  "
        - "\u0391\u03BD\u03B1\u03C0\u03C4\u03CD\u03BE\u03C4\u03B5 \u03B5\u03BD\u03CC\u03C4\u03B7\u03C4\u03B5\u03C2
