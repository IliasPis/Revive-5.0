      - text: "\u039A\u03B1\u03C4\u03AC \u03C4\u03B7 \u03B4\u03B9\u03AC\u03C1\u03BA\u03B5\u03B9\u03B1
          \u03BC\u03B9\u03B1\u03C2 \u03B5\u03BA\u03C0\u03B1\u03B9\u03B4\u03B5\u03C5\u03C4\u03B9\u03BA\u03AE\u03C2
          \u03C3\u03C5\u03BD\u03B5\u03B4\u03C1\u03AF\u03B1\u03C2, \u03BC\u03B5\u03C1\u03B9\u03BA\u03BF\u03AF
          \u03C3\u03C5\u03BC\u03BC\u03B5\u03C4\u03AD\u03C7\u03BF\u03BD\u03C4\u03B5\u03C2
          \u03BA\u03C5\u03C1\u03B9\u03B1\u03C1\u03C7\u03BF\u03CD\u03BD \u03C3\u03C4\u03B7
          \u03C3\u03C5\u03B6\u03AE\u03C4\u03B7\u03C3\u03B7, \u03B1\u03C6\u03AE\u03BD\u03BF\u03BD\u03C4\u03B1\u03C2
          \u03C4\u03BF\u03C5\u03C2 \u03C5\u03C0\u03CC\u03BB\u03BF\u03B9\u03C0\u03BF\u03C5\u03C2
          \u03B1\u03BC\u03AD\u03C4\u03BF\u03C7\u03BF\u03C5\u03C2. \u03A0\u03CE\u03C2
          \u03BC\u03C0\u03BF\u03C1\u03B5\u03AF\u03C4\u03B5 \u03BD\u03B1 \u03C4\u03BF
          \u03B1\u03BB\u03BB\u03AC\u03BE\u03B5\u03C4\u03B5 \u03B1\u03C5\u03C4\u03CC; "
        answers:
        - "\u03A4\u03B5\u03C1\u03BC\u03B1\u03C4\u03AF\u03C3\u03C4\u03B5 \u03C4\u03B7
          \u03C3\u03C5\u03B6\u03AE\u03C4\u03B7\u03C3\u03B7 \u03BD\u03C9\u03C1\u03AF\u03C2
          \u03B3\u03B9\u03B1 \u03BD\u03B1 \u03B1\u03C0\u03BF\u03C6\u03CD\u03B3\u03B5\u03C4\u03B5
          \u03C0\u03B5\u03C1\u03B1\u03B9\u03C4\u03AD\u03C1\u03C9 \u03B1\u03BD\u03B9\u03C3\u03BF\u03C1\u03C1\u03BF\u03C0\u03AF\u03B1. "
        - "\u0391\u03BD\u03B1\u03C0\u03C1\u03BF\u03C3\u03B1\u03BD\u03B1\u03C4\u03BF\u03BB\u03AF\u03C3\u03C4\u03B5
